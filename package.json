{
  "name": "3d-rl-climbing-game",
  "version": "1.0.0",
  "description": "A minimalist web-based 3D climbing game where an AI agent learns to climb using reinforcement learning",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "train": "node train.js",
    "train:ppo": "node train.js --agent PPO --episodes 1000",
    "train:dqn": "node train.js --agent DQN --episodes 1000",
    "train:fast": "node train.js --agent PPO --episodes 500 --no-trajectories"
  },
  "keywords": [
    "reinforcement-learning",
    "3d",
    "climbing",
    "ai",
    "tensorflow",
    "threejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "three": "^0.160.0",
    "cannon-es": "^0.20.0",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "chart.js": "^4.4.1",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "vite": "^5.0.11"
  }
}
